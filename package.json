  "scripts": {
    "test": "node ./src/main.js",
    "start": "node ./src/main",
    "build": "mkdir -p ../fog-controller-db && touch ../fog-controller-db/fog_controller.db && ./flyway-5.0.7/flyway -configFiles=./db/flyway.conf migrate  && mkdir -p imageSnapshot",
    "start-dev": "node ./src/daemonServer",
    "build-dev": "mkdir -p ../fog-controller-db && touch ../fog-controller-db/fog_controller.db && ./flyway-5.0.7/flyway -configFiles=./db/flyway.conf migrate && mkdir -p imageSnapshot",
    "dev": "npm start stop && npm run build-dev && npm run start-dev",
    "postinstall": "mkdir -p ../fog-controller-db && touch ../fog-controller-db/fog_controller.db && ./flyway-5.0.7/flyway -configFiles=./db/flyway.conf migrate && mkdir -p imageSnapshot"
  },
  "bin": {
    "fog-controller": "src/main.js"
  },
