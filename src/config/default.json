{
  "App": {
    "Name": "fog-controller"
  },
  "Server": {
    "Port": 54421
  },
  "Email": {
    "ActivationEnabled": false,
    "HomeUrl": "https://google.com"
  },
  "Service": {
    "LogsDirectory": "/var/log/fog-controller",
    "LogsFileSize": 1048576
  },
  "Settings": {
    "UserTokenExpirationIntervalSeconds": 3600,
    "FogTokenExpirationIntervalSeconds": 3600
  },
  "Proxy": {
    "Username": "username",
    "Password": "password",
    "Host": "127.0.0.1",
    "RsaKey": "rsa",
    "Lport": 5000,
    "PortRange": "2000-10000"
  }
}